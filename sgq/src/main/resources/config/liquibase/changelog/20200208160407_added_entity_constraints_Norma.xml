<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Norma.
    -->
    <changeSet id="20200208160407-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="anexo_id"
                                 baseTableName="norma"
                                 constraintName="fk_norma_anexo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="anexo"/>

        <addForeignKeyConstraint baseColumnNames="norma_id"
                                 baseTableName="norma_categoria"
                                 constraintName="fk_norma_categoria_norma_id"
                                 referencedColumnNames="id"
                                 referencedTableName="norma"/>

        <addForeignKeyConstraint baseColumnNames="categoria_id"
                                 baseTableName="norma_categoria"
                                 constraintName="fk_norma_categoria_categoria_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria_norma"/>
        
    </changeSet>
</databaseChangeLog>
