        <addForeignKeyConstraint baseColumnNames="anexo_id"
                                 baseTableName="item_auditoria_anexo"
                                 constraintName="fk_item_auditoria_anexo_anexo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="anexo"/>
        
    </changeSet>
</databaseChangeLog>
