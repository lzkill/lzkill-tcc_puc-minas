        <addForeignKeyConstraint baseColumnNames="anexo_id"
                                 baseTableName="campanha_recall_anexo"
                                 constraintName="fk_campanha_recall_anexo_anexo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="anexo"/>
        
    </changeSet>
</databaseChangeLog>
